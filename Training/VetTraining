//vet training using 2 pets
headmsg 'choose pet1'
promptalias 'pet1'

headmsg 'choose pet2'
promptalias 'pet2'

headmsg 'choose bandage'
promptalias 'bandage'

while not dead

useobject 'bandage'
waitfortarget 20000
target 'pet1'

useobject 'bandage'
waitfortarget 20000
target 'pet2'

pause 5000

endwhile